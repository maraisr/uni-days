    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": true
  },
  "devDependencies": {
    "@marais/tsconfig": "^0.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2",
    "vite": "^2.1.5"
