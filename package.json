{
  "name": "my-vue-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "format": "prettier --write src/**/*.{tsx,ts,css}"
  },
  "dependencies": {
    "history": "^5.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 80,
